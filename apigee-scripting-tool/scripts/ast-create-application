#!/usr/bin/env ast

# create application 

if ARGV.length != 3 then
  puts "USAGE: #{ARGV[0]} <domain> <application>"
  return 1;
end

csc = ast.configurationServiceClient

if csc.getAllApplicationNames(ARGV[1]).contains(ARGV[2]) then
  puts "Application already exists: #{ARGV[2]}"
  return 1
end

csc.createApplication(ARGV[1],ARGV[2]);

#exportDomain("./test","default")







