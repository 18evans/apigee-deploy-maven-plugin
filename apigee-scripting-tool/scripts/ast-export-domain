#!/usr/bin/env ast

# exports a domain 

if ARGV.length != 3 then
  puts "USAGE: #{ARGV[0]} <domainName> <exportFileName>"
  return 1
end

if  !ast.configurationServiceClient.getAllDomainNames().contains(ARGV[1]) then
	puts "Domain not found: #{ARGV[1]}"
	return 1
end

ast.configurationServiceClient.exportDomain(ARGV[2],ARGV[1])







